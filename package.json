{
  "name": "winston-gke-formatter",
  "version": "0.0.2",
  "description": "winston formmatter for gke(stackdriver logging)",
  "repository": {
    "type": "git",
    "url": "https://github.com/k0uki/winston-gke-formatter.git"
  },
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "mocha --recursive --require ts-node/register \"test/*.ts\"",
    "cover": "nyc npm t"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "sourceMap": true,
    "all": true
  },
  "author": "k0uki <k0uki221@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/sinon": "^7.5.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "winston": "^3.2.1"
  }
}
